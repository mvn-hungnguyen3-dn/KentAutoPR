<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Ios Test Suite" verbose="1">
    <parameter name="server" value="http://127.0.0.1:4723/wd/hub"/>
    <test name="Ios Test" enabled="true">
        <parameter name="platformName" value="iOS"/>
        <parameter name="automationName" value="XCUITest"/>
        <parameter name="deviceName" value="iPhone 7"/>
        <parameter name="udid" value=""/>
        <parameter name="platformVersion" value="12.1"/>
        <parameter name="app"
                   value=".ipa"/>
        <parameter name="noReset" value="true"/>
        <classes>
            <class name="testrunner.appium.TestRunner"/>
        </classes>
    </test>
</suite>
